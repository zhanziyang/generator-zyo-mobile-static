.page
  display: block
  width: 100%
  min-height: 100%
  background-color: $page-background-color
  position: relative
  clearfix()
  &.lg-squeezed
    width: $screezed-width
    margin: 0 auto

.mask
  absCover(fixed)
  z-index: 1000
  display: none
  background: rgba(0, 0, 0, .8)

.dialog
  display: none
  z-index: 1001
  absCenter(fixed)